<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWindowsForms>true</UseWindowsForms>
    <ImplicitUsings>enable</ImplicitUsings>
    <PackageIcon>checklisticon.png</PackageIcon>
    <ApplicationIcon>checklisticon.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="checklisticon.ico" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\checklisticon.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>